        <expression><![CDATA[PDN>0]]></expression>
    </rule>
    <!--  Cumpleaños  -->
    <rule>
        <id>Birthday</id>
        <expression><![CDATA[(NACDM==GDM)&&(NACMA==GMA)]]></expression>
    </rule>
    <!-- Cross selling accounts -->
    <rule>
        <id>segurosMovimientoCuenta</id>
        <expression><![CDATA[CMDD contains "SEGURO"]]></expression>
    </rule>
    <!-- Cross selling cuentas -->
    <rule>
        <id>segurosMovimientoTarjeta</id>
        <expression><![CDATA[TMDD contains "SEGURO"]]></expression>
    </rule>
    <!-- PG type for offers -->
    <rule>
        <id>pgClassicOffer</id>
        <expression><![CDATA[(PG_TYPE == "CLASSIC")]]></expression>
    </rule>
    <rule>
        <id>pgSimpleOffer</id>
        <expression><![CDATA[(PG_TYPE == "SIMPLE")]]></expression>
    </rule>
    <rule>
        <id>pgSmartOffer</id>
        <expression><![CDATA[(PG_TYPE == "SMART")]]></expression>
    </rule>
</rules>
