    <!--____________________________________________________  FECHAS _________________________________________________-->
    <!--   Fecha 1 (del 1 al 9)  -->
    <rule>
        <id>F0001</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=9)]]></expression>
    </rule>
    <!--  Fecha 2 (del 15 al 20)  -->
    <rule>
        <id>F0002</id>
        <expression><![CDATA[(GDM>=15)&&(GDM<=20)]]></expression>
    </rule>
    <!--  Fecha 3 (del 21 al 27)  -->
    <rule>
        <id>F0003</id>
        <expression><![CDATA[(GDM>=21)&&(GDM<=27)]]></expression>
    </rule>
    <!--  Fecha 4 (del 24 al 28)  -->
    <rule>
        <id>F0004</id>
        <expression><![CDATA[(GDM>=24)&&(GDM<=28)]]></expression>
    </rule>
    <!--  Fecha 5 (del 24 al 30)  -->
    <rule>
        <id>F0005</id>
        <expression><![CDATA[(GDM>=24)&&(GDM<=30)]]></expression>
    </rule>
    <!--  Fecha 6 (del 1 al 30)  -->
    <rule>
        <id>F0006</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=30)]]></expression>
    </rule>
    <!--   Fecha 7 (del 1 al 7)  -->
    <rule>
        <id>F0007</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=7)]]></expression>
    </rule>
    <!--   Fecha 8 (del 8 al 14)  -->
    <rule>
        <id>F0008</id>
        <expression><![CDATA[(GDM>=8)&&(GDM<=14)]]></expression>
    </rule>
    <!--   Fecha 9 (del 15 al 22)  -->
    <rule>
        <id>F0009</id>
        <expression><![CDATA[(GDM>=15)&&(GDM<=22)]]></expression>
    </rule>
    <!--   Fecha 10 (del 23 al 31)  -->
    <rule>
        <id>F0010</id>
        <expression><![CDATA[(GDM>=23)&&(GDM<=31)]]></expression>
    </rule>
    <!--   Dinero Directo   -->
    <rule>
        <id>P0002</id>
        <expression><![CDATA[PCS<200]]></expression>
    </rule>
    <rule>
        <id>6000</id>
        <expression><![CDATA[MVD contains "BANNER"]]></expression>
    </rule>
    <!--  123 autónomos   -->
    <rule>
        <id>A0002</id>
        <expression><![CDATA[PSSOCA==false]]></expression>
    </rule>
    <!--  Tiene Cuenta 123 APP  -->
    <rule>
        <id>B0001</id>
        <expression><![CDATA[PSSOC==true]]></expression>
    </rule>
    <!--   No tiene Cuenta 123 APP  -->
    <rule>
        <id>A0001</id>
        <expression><![CDATA[PSSOC==false]]></expression>
    </rule>
    <!--   No aplica a Web Tablet   -->
    <rule>
        <id>no_nwpt</id>
        <expression><![CDATA[SYS no_contains "nwpt"]]></expression>
    </rule>
    <!--   No aplica a Android   -->
    <rule>
        <id>no_android</id>
        <expression><![CDATA[SYS no_contains "android"]]></expression>
    </rule>
    <!--   No aplica a iOS   -->
    <rule>
        <id>no_ios</id>
        <expression><![CDATA[SYS no_contains "ios"]]></expression>
    </rule>
    <!--   Número de tarjetas inactivas   -->
    <rule>
        <id>ActTar</id>
        <expression><![CDATA[PTINAC>0]]></expression>
    </rule>
    <!--   Ofertas exclusivas de PB   -->
    <rule>
        <id>usuario_pb</id>
        <expression><![CDATA[(PSE == "PB")&&(PSC == "PB")]]></expression>
    </rule>
    <!--   Ofertas exclusivas de Retail   -->
    <rule>
        <id>usuario_retail</id>
        <expression><![CDATA[(PSE != "PB")||(PSC != "PB")]]></expression>
    </rule>
    <!--   Ofertas exclusivas de Smart   -->
    <rule>
        <id>usuario_smart</id>
        <expression><![CDATA[(COL_SMART_PREMIUM==true)||(COL_SMART_FREE==true)]]></expression>
    </rule>
    <!--   Ofertas exclusivas de Smart Free   -->
    <rule>
        <id>usuario_free</id>
        <expression><![CDATA[COL_SMART_FREE==true]]></expression>
    </rule>
    <!--   Ofertas exclusivas de Smart Premium   -->
    <rule>
        <id>usuario_premium</id>
        <expression><![CDATA[COL_SMART_PREMIUM==true]]></expression>
    </rule>
    <!--   Vista Smart   -->
    <rule>
        <id>ui_smart</id>
        <expression><![CDATA[UI_STYLE==smart]]></expression>
    </rule>
    <!-- Oferta para clientes que tengan en sus posiciones la cuenta mini  -->
    <rule>
        <id>reglamini</id>
